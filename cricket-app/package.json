{
  "name": "cricket-app",
  "version": "1.0.0",
  "homepage": "/cricketers/",
  "private": true,
  "scripts": {
    "start": "./node_modules/.bin/react-scripts start -FAST_REFRESH=true",
    "build": "./node_modules/.bin/react-scripts build && cp ./build/service-worker.js ./public/service-worker.js",
    "test": "./node_modules/.bin/react-scripts test",
    "eject": "./node_modules/.bin/react-scripts eject",
    "test:coverage": "./node_modules/.bin/react-scripts test --coverage --reporters=default --reporters=jest-junit",
    "test:coverage:dev": "./node_modules/.bin/react-scripts test --coverage --reporters=default --reporters=jest-junit --watchAll",
    "messages:extract": "formatjs extract 'node_modules/@sd-player/**/*.{ts,tsx}' 'src/**/*.{ts,tsx}' --ignore 'src/react-app-env.d.ts' --out-file public/i18n/en-extracted.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "messages:compile": "formatjs compile public/i18n/en-extracted.json --out-file public/i18n/en-us.json --format formatter.js && formatjs compile public/i18n/en-extracted.json --out-file public/i18n/en-gb.json --format formatter.js && formatjs compile public/i18n/en-extracted.json --out-file public/i18n/en.json --format formatter.js"
  },
  "dependencies": {
    "@testing-library/dom": "^6.11.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/node": "^10.12.2",
    "@types/react": "^17.0.13",
    "@types/react-burger-menu": "^2.6.1",
    "@types/react-dom": "^17.0.14",
    "@types/react-router-dom": "^5.1.7",
    "antd": "^3.26.9",
    "clsx": "^1.1.1",
    "core-js": "3.0",
    "date-fns": "^2.16.1",
    "date-fns-tz": "^1.1.1",
    "dompurify": "^2.0.12",
    "history": "^4.7.2",
    "intl-messageformat-parser": "^6.0.11",
    "jest-junit": "^6.3.0",
    "lodash": "^4.17.11",
    "node-sass": "^4.9.4",
    "prop-types": "^15.7.2",
    "react": "^18.0.0",
    "react-app-polyfill": "^0.1.3",
    "react-burger-menu": "^2.7.1",
    "react-device-detect": "^2.2.2",
    "react-dom": "^18.0.0",
    "react-draggable": "^4.4.4",
    "react-ga": "^3.3.0",
    "react-https-redirect": "^1.1.0",
    "react-intl": "^5.8.8",
    "react-jss": "^10.8.2",
    "react-resize-panel-ts": "^0.0.5",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-sidebar": "^3.0.2",
    "react-toastify": "8.0.0",
    "typescript": "^3.7.4",
    "usehooks-ts": "^1.2.0",
    "web-vitals": "^2.1.4",
    "workbox-background-sync": "^5.1.3",
    "workbox-broadcast-update": "^5.1.3",
    "workbox-cacheable-response": "^5.1.3",
    "workbox-core": "^5.1.3",
    "workbox-expiration": "^5.1.3",
    "workbox-google-analytics": "^5.1.3",
    "workbox-navigation-preload": "^5.1.3",
    "workbox-precaching": "^5.1.3",
    "workbox-range-requests": "^5.1.3",
    "workbox-routing": "^5.1.3",
    "workbox-strategies": "^5.1.3",
    "workbox-streams": "^5.1.3"
  },
  "devDependencies": {
    "@formatjs/cli": "^3.2.0",
    "@formatjs/icu-messageformat-parser": "^1.2.1",
    "@formatjs/ts-transformer": "^2.12.0",
    "@types/dompurify": "^2.0.2",
    "@types/jest": "^26.0.23",
    "babel-plugin-react-intl": "^8.2.10",
    "eslint-plugin-formatjs": "^2.12.8",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "^3.6.0",
    "typescript-tslint-plugin": "^0.3.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
